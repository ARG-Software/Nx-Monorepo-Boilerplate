    "next": "^15.1.6",
    "passport-jwt": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.14",
    "tailwindcss": "^3.4.17",
    "winston": "^3.17.0",
    "winston-errsole": "^1.0.0"
  },
  "devDependencies": {
    "@jscutlery/semver": "^5.5.1",
    "@mikro-orm/cli": "^6.4.4",
