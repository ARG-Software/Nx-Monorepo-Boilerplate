    "@types/jest": "29.5.14",
    "@types/node": "~18.16.20",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "18.2.33",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "29.7.0",
    "eslint": "^9.19.0",
