{
  "name": "@nx-template/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:frontend": "nx serve frontend --verbose",
    "start:api": "nx serve client-api --verbose",
    "start:action-log-service": "nx serve action-log-service --verbose",
    "start:frontend:api": "nx run-many --target=serve --projects=frontend,client-api --parallel",
    "start:all": "nx run-many --target=serve --projects=client-api,frontend,action-log-service --parallel",
    "build:all": "nx run-many --target=build --configuration=production --projects=frontend,client-api,action-log-service",
    "build:frontend": "nx build frontend --configuration=production",
    "build:api": "nx build client-api --configuration=production",
    "build:action-log-service": "nx build action-log-service --configuration=production",
    "docker:generate:all": "nx run-many --target=generate-docker-image --configuration=production --projects=frontend,client-api,action-log-service",
    "docker:generate:frontend": "nx run frontend:generate-docker-image --configuration=production",
    "docker:publish:affected": "nx affected --target=publish-docker --configuration=production --no-cloud --parallel=1 --projects=frontend,client-api,action-log-service",
    "docker:publish:frontend": "nx run frontend:publish-docker --configuration=production",
    "docker:publish:api": "nx run client-api:publish-docker --configuration=production",
    "docker:publish:strategies-service": "nx run action-log-service:publish-docker --configuration=production",
    "docker:publish:all": "nx run-many --target=publish-docker --configuration=production --projects=frontend,client-api,action-log-service",
    "migration:action-log:generate": "mikro-orm migration:create --config ./tools/database/mikro-orm.action-log.config.ts ",
    "migration:action-log:up": "mikro-orm migration:up --config ./tools/database/mikro-orm.action-log.config.ts",
    "migration:action-log:down": "mikro-orm migration:down --config ./tools/database/mikro-orm.action-log.config.ts",
    "migration:main:generate": "mikro-orm migration:create --config ./tools/database/mikro-orm.main.config.ts",
    "migration:main:up": "mikro-orm migration:up --config ./tools/database/mikro-orm.main.config.ts",
    "migration:main:down": "mikro-orm migration:down --config ./tools/database/mikro-orm.main.config.ts",
    "dump:main:create": "mikro-orm schema:create --schema app --config ./tools/database/mikro-orm.main.config.ts --dump > ./tools/database/dumps/main/$(date \"+%Y-%m-%d_%H-%M-%S\")main-schema.sql",
    "dump:main:update": "mikro-orm schema:update --schema app --config ./tools/database/mikro-orm.main.config.ts --dump > ./tools/database/dumps/main/$(date \"+%Y-%m-%d_%H-%M-%S\")main-schema.sql",
    "dump:action-log:create": "mikro-orm schema:create --schema action-log --config ./tools/database/mikro-orm.action-log.config.ts --dump > ./tools/database/dumps/action-log/$(date \"+%Y-%m-%d_%H-%M-%S\")action-log-schema.sql",
    "dump:action-log:update": "mikro-orm schema:update --schema action-log --config ./tools/database/mikro-orm.action-log.config.ts --dump > ./tools/database/dumps/action-log/$(date \"+%Y-%m-%d_%H-%M-%S\")action-log-schema.sql"
  },
  "private": true,
  "dependencies": {
    "@mikro-orm/core": "^6.4.4",
    "@mikro-orm/nestjs": "^6.1.0",
    "@mikro-orm/postgresql": "^6.4.4",
    "@mikro-orm/reflection": "^6.4.4",
    "@mui/material": "^5.16.14",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/cache-manager": "^2.3.0",
