    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "filemanager-webpack-plugin": "^8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "20.0.10",
    "postcss": "^8.5.1",
    "prettier": "^2.8.8",
    "sass": "^1.83.4",
    "testcontainers": "^10.17.2",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "tslib": "^2.8.1",
    "typescript": "5.5.4",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "@nx/nx-linux-x64-gnu": "20.0.10",
    "@nx/nx-linux-x64-musl": "20.0.10",
    "@nx/nx-win32-x64-msvc": "20.0.10"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.mikrorm.json",
    "configPaths": [
      "./tools/database/mikro-orm.main.config.ts",
      "./tools/database/mikro-orm.action-log.config.ts",
      "./tools/database/mikro-orm.webhook.config.ts"
    ]
  },
  "overrides": {
    "html-minifier": "5.1.0"
  }
}
