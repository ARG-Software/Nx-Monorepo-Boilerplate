    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.3.0",
    "@tanstack/react-query": "^5.65.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie": "^1.0.2",
    "errsole": "^2.12.0",
    "errsole-postgres": "^2.3.0",
    "express": "^4.21.2",
    "framer-motion": "^11.18.2",
